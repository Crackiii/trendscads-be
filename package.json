{
  "name": "trendscads-be",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "build-ts": "tsc",
    "build": "npm run build-ts && npm run lint",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "dev": "npm run build && npm run watch",
    "start": "npm install && npm run build && npm run serve",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Crackiii/trendscads-be.git"
  },
  "author": "Nadeem Ahmad",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Crackiii/trendscads-be/issues"
  },
  "homepage": "https://github.com/Crackiii/trendscads-be#readme",
  "dependencies": {
    "@prisma/client": "^3.12.0",
    "axios": "^0.26.1",
    "express": "^4.17.3",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.182",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "concurrently": "^7.1.0",
    "eslint": "^8.13.0",
    "nodemon": "^2.0.15",
    "prisma": "^3.12.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
